apiVersion: networking.k8s.io/v1beta1
kind: Ingress
metadata:
  name: grafana-ingress
  annotations:
    nginx.ingress.kubernetes.io/rewrite-target: /
spec:
  rules:
  - host: {{ grafana_url }}
    http:
      paths:
      - path: /
        backend:
          serviceName: prometheus-operator-grafana 
          servicePort: 80
